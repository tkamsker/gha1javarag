<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:bite="urn:import:at.a1ta.bite.ui.client.widget">
     <ui:with field="msg" type="at.a1ta.cuco.admin.ui.common.client.bundle.AdminCommonTextPool"/>
     <ui:with field='style' type='at.a1ta.bite.ui.client.bundle.StyleResources'/>
     <ui:with field='img' type='at.a1ta.bite.ui.client.bundle.images.ImageResources'/>
	
	<g:HTMLPanel>
	<bite:Label text="{msg.serviceDialogHeading}:" />
	 
	 <div class="{style.bite_moduleHeader}" style="padding-top:5px;" />
      <div class="{style.bite_moduleContent}">
      <bite:Label text="{msg.serviceDialogCpServicedata}:" />
      <table class="{style.bite_form}">
      <colgroup>
          <col width="20%" />
          <col width="30%" />
          <col width="20%" />
          <col width="30%" />
        </colgroup>
        <tr>
        <td><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogFromLabel}:"/></td>
        <td><bite:DateBox ui:field="from" size="Short" /></td>
        <td><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogToLabel}:"/> </td>
        <td><bite:DateBox ui:field="to" size="Short" /></td>
        </tr>
       <tr>
       <td><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogNameLabel}:"/></td>
       <td colspan="3"><bite:InputBox ui:field="name" size="Long" mandatoryValidation="true" validationErrorMessage="{msg.serviceDialogValidation}" /></td>
       </tr>
       <tr>
       <td><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogDescription}:"/></td>
       <td colspan="3"><bite:TextArea ui:field="description" width="100%" height="40px" /></td>
       </tr> 
       <tr>
        <td><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogEmployeeinfo}:"/></td>
        <td colspan="3"><bite:TextArea ui:field="employeeinfo" width="100%" height="40px"/></td>
       </tr> 
      </table>
      </div>
      
      <div class="{style.bite_moduleHeader}" style="padding-top:5px;" />
      <div class="{style.bite_moduleContent}">
      <bite:Label text="{msg.serviceDialogCpChargingdata}:" />
      <table class="{style.bite_form}">
      <colgroup>
          <col width="17%" />
          <col width="5%" />
          <col width="1%" />
          <col width="34%" />
          <col width="5%" />
          <col width="25%" />
          <col width="13%" />
        </colgroup>
         <tr>
         	<td><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogCostsLabel}:"/></td>
         	<td><bite:InputBox ui:field="cost" size="Short" mandatoryValidation="true" validationErrorMessage="{msg.serviceDialogValidation}" /></td>
         	<td><bite:Icon ui:field="iconInfo" img="{img.iconInfoTiny}"/></td>
         	<td class="{style.center}"><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogMultiLabel}"/></td>
         	<td><bite:InputBox ui:field="multi" size="Short"/></td>
         	<td class="{style.center}"><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogProductcodeLabel}:"/></td>
         	<td><bite:InputBox ui:field="productCode" size="Medium" mandatoryValidation="true" validationErrorMessage="{msg.serviceDialogValidation}" /></td>
         </tr>
      </table>
      <table class="{style.bite_form}">
      <colgroup>
          <col width="20%" />
          <col width="30%" />
          <col width="20%" />
          <col width="30%" />
        </colgroup>
         <tr>
         	<td><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogChargingtypeLabel}:"/></td>
         	<td><bite:ListBox ui:field="chargingType" width="100%"/></td>
         	<td class="{style.center}"><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogCredittypeLabel}:"/></td>
         	<td><bite:ListBox ui:field="creditType" width="100%"/></td>
         </tr>
      </table>
      </div>
      
      <div class="{style.bite_moduleHeader}" style="padding-top:5px;" />
      <div class="{style.bite_moduleContent}">
      <bite:Label text="{msg.serviceDialogCpClarifydata}:" />
      <table class="{style.bite_form}">
      <colgroup>
          <col width="20%" />
          <col width="80%" />
      </colgroup>
         <tr>
         <td><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogProductcodeLabel}:"/></td>
         <td><bite:InputBox ui:field="product" size="Long" /></td>
         </tr>
         <tr>
         <td><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogReason1Label}:"/></td>
         <td><bite:InputBox ui:field="reason1" size="Long" /></td>
         </tr>
         <tr>
         <td><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogReason2Label}:"/></td>
         <td><bite:InputBox ui:field="reason2" size="Long" /></td>
         </tr>
         <tr>
         <td><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogReason3Label}:"/></td>
         <td> <bite:InputBox ui:field="reason3" size="Long" /></td>
         </tr>
         <tr>
         <td><bite:Label styleName="{style.bite_text}" text="{msg.serviceDialogResultLabel}:"/></td>
         <td><bite:InputBox ui:field="result" size="Long" /></td>
         </tr>
      </table>
      </div>
      
      <div class="{style.floating_right}">
          <bite:Button ui:field="bSave" text="{msg.dialogSave}" size="Small"/>
          <bite:Button ui:field="bCancel" text="{msg.dialogCancel}" size="Small"/>
      </div>
      <div class="{style.floating_clear}" />
	</g:HTMLPanel>
</ui:UiBinder>
