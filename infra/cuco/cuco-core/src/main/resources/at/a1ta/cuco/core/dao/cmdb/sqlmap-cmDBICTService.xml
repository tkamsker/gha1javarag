<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
  PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
  "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="CmDBICTService">
  <cacheModel id="CmDBICTService-cache" type="ehcacheProvider" readOnly="true" serialize="false">
	<flushInterval hours="1" />
	<property name="cache-size" value="1" />
  </cacheModel>

  <resultMap id="cmDBICTService-result" class="at.a1ta.cuco.core.shared.dto.product.PartySummaryItem">
	<result property="name" column="servicename" />
	<result property="url" column="deeplink" />
  </resultMap>

  <select id="getICTServicesForPartyId" resultMap="cmDBICTService-result" parameterClass="java.lang.String">
	select * from dbo.export_cuco where kums_id =#partyId#
  </select>

</sqlMap>
